<?xml version="1.0" encoding="utf-8"?>
<!--
  AI-Box NuGet Configuration
  Pre-configured to use the Nexus repository mirror at nexus.internal.
  All package restores will be served from the internal mirror, avoiding
  direct access to nuget.org.
-->
<configuration>
  <packageSources>
    <clear />
    <add key="nexus" value="https://nexus.internal/repository/nuget-group/index.json" />
  </packageSources>
  <packageSourceMapping>
    <packageSource key="nexus">
      <package pattern="*" />
    </packageSource>
  </packageSourceMapping>
</configuration>
